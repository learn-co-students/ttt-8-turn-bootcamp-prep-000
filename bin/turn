#!/usr/bin/env ruby
require_relative '../lib/turn'
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
move(board, 4, "X")
move(board, 0, "O")
puts "Welcome to Tic Tac Toe!"
display_board(board)
# run the game 9 times
turn(board)
turn(board)
turn(board)
turn(board)
turn(board)
turn(board)
turn(board)
turn(board)
turn(board)
